{
    "name": "isaacdew/overland-framework",
    "description": "A small framework for writing WordPress REST API routes in the Laravel style.",
    "keywords": ["api", "wordpress"],
    "minimum-stability": "stable",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Isaac Dew",
            "email": "info@isaacdew.com"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Overland\\Tests\\": "tests/"
        }
    },
    "autoload": {
        "files": ["bootstrap.php"],
        "psr-4": {
            "Overland\\": "src/"
        }
    },
    "require": {
        "php":"^8.0",
        "firebase/php-jwt": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "wordpress/wordpress": "dev-master",
        "yoast/phpunit-polyfills": "^1.0"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/WordPress/wordpress-develop/"
        }
    ]
}
